                                <span class="form-for-button__placeholder-for-button">Куда</span>
                                <div class="form-for-button__arrow-for-button"></div>
                                </button>
                                <div class="form-for-button__the-selection-panel">
                                    <div class="list-group">
                                        <% routes.forEach(route => { %>
                                        <label class="list-group-item">
                                            <input class="form-check-input me-1" type="checkbox" value= '<%= route.route_id %>'>
                                            <%= route.name %>
                                        </label> 
                                        <% }); %>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-for-search__wrap-for-button">
                            <span class="form-for-search__label-for-button">Дата отправления</span>
                            <input type="text" id="datepicker" class="form-for-search__datepicker">
                            <script>
                            var datepicker = new Datepicker('#datepicker',{ranged: true, min: (function(){
                                var date = new Date();
                                date.setDate(date.getDate());
                                return date;
                            })()});
                            </script>
                        </div>
                        <div class="form-for-search__wrap-for-button">
                            <span class="form-for-search__label-for-button">Дней в круизе</span>
                            <div class="form-for-search__wrap-for-button-and-panel">
                                
                                <button class="form-for-search__button-for-selecting">
                                <span class="form-for-button__placeholder-for-button">Как долго</span>
                                <div class="form-for-button__arrow-for-button"></div>
                                </button>
                                <div class="form-for-button__the-selection-panel">
                                    <div class="list-group">
                                        <label class="list-group-item">
                                            <input class="form-check-input me-1" type="checkbox" value="1">
                                            1 - 4 дня
                                        </label>
                                        <label class="list-group-item">
                                            <input class="form-check-input me-1" type="checkbox" value="2">
                                            5 - 7 дней
                                        </label>
                                        <label class="list-group-item">
                                            <input class="form-check-input me-1" type="checkbox" value="3">
                                            8 - 10 дней
                                        </label>
                                        <label class="list-group-item">
                                            <input class="form-check-input me-1" type="checkbox" value="4">
                                            11 - 14 дней
                                        </label>
                                        <label class="list-group-item">
                                            <input class="form-check-input me-1" type="checkbox" value="5">
                                            более 14 дней
                                        </label>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-for-search__wrap-for-button">
                            <span class="form-for-search__label-for-button">Теплоход</span>
                            <div class="form-for-search__wrap-for-button-and-panel">
                                
                                <button class="form-for-search__button-for-selecting">
                                <span class="form-for-button__placeholder-for-button">На чем</span>
                                <div class="form-for-button__arrow-for-button"></div>
                                </button>
                                <div class="form-for-button__the-selection-panel">
