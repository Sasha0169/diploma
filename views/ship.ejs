    <div class="body page__body">
        <div class="ship-page">
            <h1 class="ship-page__ship-name">
                Теплоход "Александр Радищев"
            </h1>
            <div class="ship-page__content">
                <div class="ship-page__general-information">
                    <div class="block-of-images">
                        <img src="/photos/ships/1/main.jpg"
                            class="block-of-images__main-image">
                        <div class="block-of-images__images">
                            <img src="/left-arrow-for-images.png" class="block-of-images__left-arrow">
                            <div class="block-of-images__wrap-for-images">
                                <img class="block-of-images__image" src="/photos/ships/1/main.jpg">
                                <img class="block-of-images__image" src="/photos/ships/1/main.jpg">
                                <img class="block-of-images__image" src="/photos/ships/1/main.jpg">
                                <img class="block-of-images__image" src="/photos/ships/1/main.jpg">
                            </div>
                            <img src="/right-arrow-for-images.png" class="block-of-images__right-arrow">
                        </div>
                    </div>
                    <div class="ship-page__wrap-for-characteristics-panel characteristics-panel">
                        <span class="characteristics-panel__title">Основные характеристики</span>
                        <div class="characteristics-panel__wrap-for-characteristic">
                            <% for(let i=0; i<ship.technical_info.length; i++){ %>
                                <div class="characteristics-panel__characteristic">
                                    <span class="characteristics-panel__characteristic-name"><%= ship.technical_info[i].name %></span>
                                    <span class="characteristics-panel__characteristic-value"><%= ship.technical_info[i].value %></span>
                                </div>
                            <% } %>
                        </div>
                    </div>
                </div>
                <div class="ship-page__sections">
                    <div class="page-navigation">
                        <a href="#section1" class="page-navigation__section">
                            <div class="page-navigation__name-of-section">
                                Описание теплохода
                            </div>
                        </a>
                        <a href="#section2" class="page-navigation__section">
                            <div class="page-navigation__name-of-section">
                                Каюты
                            </div>
                        </a>
                        <a href="#section3" class="page-navigation__section">
                            <div class="page-navigation__name-of-section">
                                Удобства на борту
                            </div>
                        </a>
                        <a href="#section4" class="page-navigation__section">
                            <div class="page-navigation__name-of-section">
                                Круизы
                            </div>
                        </a>
                    </div>
                    <div id="section1" class="ship-page__section ship-page-section">
                        <h2 class="ship-page-section__name-of-section">
                            Описание теплохода
                        </h2>
                        <div class="ship-page-section__content">
                            <%- ship.ship_description.longDescription %>
                        </div>
                    </div>
                    <div id="section2" class="ship-page__section ship-page-section">
                        <h2 class="ship-page-section__name-of-section">
                            Каюты
                        </h2>
                        <div class="ship-page-section__content">
                            <div class="wrap-for-cabin-prices cabin-prices">
                                <% for(let i=0; i<ship.ship_decks.length; i++){ %>
                                    <div class="cabin-prices__deсk">
                                        <span class="cabin-prices__deck-name">
                                            <%= ship.ship_decks[i].deck_name %>
                                        </span>
                                        <div class="cabin-prices__wrap-for-cabins">
                                            <% for(let j=0; j<ship.ship_decks[i].cabins.length; j++){ %>
                                                <div class="cabin-prices__cabin cabin cabin_for-ship">
                                                    <div class="cabin__upper-part">
                                                        <img src="/cabin.jpeg" alt="" class="cabin__image">
                                                        <div class="cabin__wrap-for-information">
                                                            <span class="cabin__name">
                                                                <%= ship.ship_decks[i].cabins[j].cabin_name %>
                                                            </span>
                                                            <div class="cabin__describe">
                                                                <%= ship.ship_decks[i].cabins[j].cabin_description %>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="cabin__lower-part">
                                                        <span class="cabin__label-under-available-places">
                                                            Каюты
                                                        </span>
                                                        <div class="cabin__wrap-for-available-places">
                                                            <% for(let k=0; k<ship.ship_decks[i].cabins[j].cabin_numbers.length; k++){%>
                                                                <div class="cabin__available-place">
                                                                    <%= ship.ship_decks[i].cabins[j].cabin_numbers[k] %>
                                                                </div>
                                                            <% } %>        
                                                        </div>
                                                        <%- ship.ship_decks[i].deck_scheme %>
                                                    </div>
                                                </div>
                                                <% } %>
                                        </div>
                                    </div>
                                    <% } %>
                            </div>
                        </div>
                    </div>
                    <div id="section3" class="ship-page__section ship-page-section">
                        <h2 class="ship-page-section__name-of-section">
                            Удобства на борту
                        </h2>
                        <div class="ship-page-section__content">
                            <ul class="ship-page-section list">
                                <% for(let k=0; k<ship.services.length; k++){%>
                                    <li class="list__item">
                                        <%= ship.services[k].replaceAll("'", "") %>
                                    </li>
                                <% } %>
                            </ul>
                        </div>
                    </div>
                    <div id="section4" class="ship-page__section ship-page-section">
                        <h2 class="ship-page-section__name-of-section">
                            Круизы
                        </h2>
                        <div class="ship-page-section__content">
                            <div class="wrap-for-cruise-cards">

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
